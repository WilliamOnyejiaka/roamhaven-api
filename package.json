  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "postgres": "npx prisma generate --schema=prisma/postgres/schema.prisma",
    "mongo": "npx prisma generate --schema=prisma/mongo/schema.prisma",
    "change": "npx prisma generate --schema=prisma/postgres/schema.prisma && npx prisma migrate dev --schema=prisma/postgres/schema.prisma --name init --create-only",
    "build": "tsc -p . && npm run copy-views",
    "copy-views": "cp -R src/views dist/views",
    "migrate": "npx prisma migrate deploy --schema=prisma/postgres/schema.prisma",
    "db-reset": "npx prisma migrate reset --schema=prisma/postgres/schema.prisma --force",
    "format": "npx prisma format",
    "cli": "ts-node src/console/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
